<assets></assets>
<header class="hide-moxi-bar-mobile"></header>

<script src="https://base-devint.moxiworks.com/assets/jquery.chromebar.js"></script>
<script>
  $(document).ready(function() {
    // load the moxi bar
    var data = $('body').data();
    new WmsChromebar({
      chrome_url: data.chromeAppUrl,
      auth_url: data.servicebase + '/service/' + data.serviceversion + '/auth',
      current_module: 'account_manager',
      include_assets: true,
      company_uuid: data.companyUuid,
      account_access_redirect: data.rosterservicebase,
      callback: function(assets, header, footer) {
        $('assets').html(assets);
        $('header').html(header);
        WmsChromebar.adjustHeader()
      }
    });
  });
</script>

